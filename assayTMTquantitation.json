    },
    "batchChannel": {
      "$ref": "cmolitor.test-annotations.batchChannel"
    },
    "controlType": {
      "$ref": "cmolitor.test-annotations.controlType"
    },
    "fragmentation": {
      "$ref": "cmolitor.test-annotations.fragmentation"
    },
    "multiconsensusStudyFileID": {
      "$ref": "cmolitor.test-annotations.multiconsensusStudyFileID"
    },
    "resolution": {
      "$ref": "cmolitor.test-annotations.resolution"
    },
    "sampleID": {
      "$ref": "cmolitor.test-annotations.sampleID"
