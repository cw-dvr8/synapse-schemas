{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "rnaSeq Assay Validation Schema",
  "$id":"cmolitor.test-assayValidation.rnaSeq",
  "description": "Validates PsychENCODE RNASeq assay metadata",
  "allOf": [
      "$reg": "cmolitor.text-assayValidation.base_assay"
  ],
  "properties": {
    "isStranded": {
      "$ref": "cmolitor.test-annotations.isStranded"
    },
    "libraryBatch": {
      "$ref": "cmolitor.test-annotations.libraryBatch"
    },
    "libraryID": {
      "$ref": "cmolitor.test-annotations.libraryID"
    },
    "libraryPrep": {
      "$ref": "cmolitor.test-annotations.libraryPrep"
    },
    "libraryPreparationMethod": {
      "$ref": "cmolitor.test-annotations.libraryPreparationMethod"
    },
    "medianGenes": {
      "$ref": "cmolitor.test-annotations.medianGenes"
    },
    "medianUMIs": {
      "$ref": "cmolitor.test-annotations.medianUMIs"
    },
    "numberCells": {
      "$ref": "cmolitor.test-annotations.numberCells"
    },
    "readLength": {
      "$ref": "cmolitor.test-annotations.readLength"
    },
    "readStrandOrigin": {
      "$ref": "cmolitor.test-annotations.readStrandOrigin"
    },
    "rnaBatch": {
      "$ref": "cmolitor.test-annotations.rnaBatch"
    },
    "runType": {
      "$ref": "cmolitor.test-annotations.runType"
    },
    "sequencingBatch": {
      "$ref": "cmolitor.test-annotations.sequencingBatch"
    },
    "totalReads": {
      "$ref": "cmolitor.test-annotations.totalReads"
    },
    "validBarcodeReads": {
      "$ref": "cmolitor.test-annotations.validBarcodeReads"
    }
  }
}
